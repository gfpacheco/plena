{
  "expo": {
    "name": "Plena",
    "slug": "plena",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffefd6"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gfpacheco.plena"
    },
    "android": {
      "package": "com.gfpacheco.plena"
    },
    "androidStatusBar": {
      "backgroundColor": "#f4b382",
      "barStyle": "dark-content"
    },
    "androidNavigationBar": {
      "backgroundColor": "#f4b382",
      "barStyle": "dark-content"
    }
  }
}
